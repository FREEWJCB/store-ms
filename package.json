    "class-validator": "^0.14.1",
    "fastify": "^5.2.2",
    "nestjs-i18n": "^10.5.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
